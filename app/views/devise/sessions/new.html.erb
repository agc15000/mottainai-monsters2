<h2>ログイン</h2>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "ログイン" %>
<% end %>
<%= link_to "パスワードを忘れた方はこちら", new_password_path(resource_name) %>
<%= link_to "新規登録", new_registration_path(resource_name) %>